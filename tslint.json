{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-clean-code"
  ],
  "jsRules": {},
  "rules": {
    "ordered-imports": [
      true,
      {
        "import-sources-order": "any",
        "named-imports-order": "case-insensitive"
      }
    ],
    "no-console": [false],
    "max-line-length": [false],
    "no-var-requires": false,
    "object-literal-sort-keys": [
      true,
      "ignore-blank-lines",
      "ignore-case",
      "locale-compare",
      "match-declaration-order",
      "shorthand-first"
    ]
  },
  "rulesDirectory": []
}
